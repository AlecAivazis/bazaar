{
    "name": "server",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "NODE_ENV=test jest",
        "test:watch": "npm run test -- --watch",
        "test:coverage": "npm run test -- --coverage",
        "test:coverage:report": "npm run test:coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "start": "babel-node index.js",
        "__internal__knex": "knex --knexfile database/knexfile.js",
        "db:migrate": "npm run __internal__knex -- migrate:latest",
        "db:seed": "npm run __internal__knex -- seed:run",
        "db:init": "npm run db:migrate && npm run db:seed"
    },
    "keywords": [],
    "author": "Alec Aivazis",
    "license": "ISC",
    "proxy": "http://localhost:4000",
    "prettier": {
        "tabWidth": 4,
        "printWidth": 100,
        "semi": false,
        "singleQuote": true,
        "trailing-comma": true
    },
    "jest": {
        "testMatch": [
            "**/*test.js?(x)"
        ]
    },
    "dependencies": {
        "apollo-fetch": "^0.6.0",
        "apollo-server-express": "^1.2.0",
        "babel-cli": "^6.26.0",
        "babel-jest": "^21.2.0",
        "babel-plugin-root-import": "^5.1.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-0": "^6.24.1",
        "body-parser": "^1.18.2",
        "dotenv": "^4.0.0",
        "express": "^4.16.2",
        "graphql": "^0.11.7",
        "graphql-tools": "^2.7.2",
        "isomorphic-fetch": "^2.2.1",
        "join-monster": "^2.0.15",
        "join-monster-graphql-tools-adapter": "0.0.2",
        "knex": "^0.14.0",
        "query-string": "^5.0.1",
        "regenerator-runtime": "^0.11.0",
        "sqlite": "^2.8.0"
    },
    "devDependencies": {
        "coveralls": "^3.0.0",
        "jest": "^21.2.1",
        "knex-cleaner": "^1.1.3",
        "moment": "^2.19.2"
    }
}
