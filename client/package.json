{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "babel-plugin-relay": "^1.4.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "graphql-cli": "^1.1.0",
        "quark-web": "0.0.9-beta.33",
        "react": "^16.0.0",
        "react-app-rewired": "^1.3.5",
        "react-dom": "^16.0.0",
        "react-native-web": "^0.1.13",
        "react-relay": "^1.4.1",
        "react-router-dom": "^4.2.2",
        "react-scripts": "1.0.17",
        "relay-compiler": "^1.4.1"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "semi": false,
        "singleQuote": true,
        "trailing-comma": true
    },
    "scripts": {
        "start": "react-app-rewired start",
        "build": "react-app-rewired build",
        "build:schema": "graphql get-schema dev",
        "test": "react-app-rewired test --env=jsdom",
        "eject": "react-scripts eject",
        "relay": "relay-compiler --src ./src --schema schema.json",
        "relay:watch": "npm run relay -- --watch"
    }
}
